d: 1.6;\n    \n    /* Effects */\n    --box-shadow: 0 4px 24px rgba(0, 0, 0, 0.08);\n    --box-shadow-sm: 0 2px 8px rgba(0, 0, 0, 0.05);\n    --transition: all 0.2s ease;\n    \n    /* Grays */\n    --gray-50: #f8f9fa;\n    --gray-100: #f1f3f4;\n    --gray-200: #e9ecef;\n    --gray-300: #dee2e6;\n    --gray-400: #9ca3af;\n    --gray-500: #6c757d;\n    --gray-600: #495057;\n    --gray-700: #333;\n}\n/* ================================ */\n/* UTILITY CLASSES                  */\n/* ================================ */\n.opaque {\n    opacity: 1;\n}\n.transparent {\n    opacity: 0;\n}\n.no-padding-right {\n    padding-right: 0 !important;\n    margin-right: 0 !important;\n}\n/* ================================ */\n/* LAYOUT & FLEX UTILITIES          */\n/* Small set of helpers for common row/column patterns used across the app */\n/* ================================ */\n/* Row: horizontal layout with gap and wrapping */\n.row {\n    display: flex;\n    gap: 12px;\n    align-items: center;\n    flex-wrap: wrap;\n}\n/* Column (vertical) layout */\n.col {\n    display: flex;\n    flex-direction: column;\n    gap: 12px;\n}\n/* Center content both axes */\n.center {\n    display: flex;\n    align-items: center;\n    justify-content: center;\n}\n/* Space-between helper */\n.space-between {\n    display: flex;\n    justify-content: space-between;\n    align-items: center;\n}\n/* Allow element to take remaining space */\n.flex-1 {\n    flex: 1 1 0%;\n}\n/* Muted text helper (alias for gray color) */\n.muted {\n    color: var(--gray-600);\n}\n/* Text color helpers */\n.text-primary {\n    color: var(--primary-color) !important;\n}\n.text-secondary {\n    color: var(--secondary-color) !important;\n}\n/* ================================ */\n/* CARD UTILITY                     */\n/* Reusable card style for demo and app components */\n/* ================================ */\n.card {\n    background: white;\n    border: 1px solid var(--gray-200);\n    border-radius: var(--border-radius-lg);\n    padding: calc(var(--space-4));\n    box-shadow: var(--box-shadow-sm);\n}\n/* Slightly stronger elevation when needed */\n.card--elevated {\n    box-shadow: var(--box-shadow);\n}\n/* ================================ */\n/* SWATCH / PALETTE HELPERS        */\n/* Small helpers to render color swatches in docs and components */\n/* ================================ */\n.palette {\n    display: flex;\n    gap: 12px;\n    flex-wrap: wrap;\n    align-items: flex-start;\n}\n.swatch {\n    width: 120px;\n    border-radius: 8px;\n    overflow: hidden;\n    border: 1px solid var(--gray-200);\n    background: white;\n}\n.swatch__color {\n    height: 72px;\n    width: 100%;\n}\n.swatch__label {\n    padding: 8px 10px;\n    font-size: var(--text-sm);\n    color: var(--gray-700);\n    background: #fff;\n}\n/* ================================ */\n/* TYPOGRAPHY SYSTEM                */\n/* ================================ */\n/* Base typography utilities */\n.text-xs {\n    font-size: var(--text-xs);\n    line-height: var(--leading-normal);\n}\n.text-sm {\n    font-size: var(--text-sm);\n    line-height: var(--leading-normal);\n}\n.text-base {\n    font-size: var(--text-base);\n    line-height: var(--leading-normal);\n}\n.text-lg {\n    font-size: var(--text-lg);\n    line-height: var(--leading-normal);\n}\n.text-xl {\n    font-size: var(--text-xl);\n    line-height: var(--leading-tight);\n}\n.text-2xl {\n    font-size: var(--text-2xl);\n    line-height: var(--leading-tight);\n}\n.text-3xl {\n    font-size: var(--text-3xl);\n    line-height: var(--leading-tight);\n}\n/* Semantic typography classes */\n.text-caption {\n    font-size: var(--text-xs);\n    line-height: var(--leading-normal);\n    color: var(--gray-500);\n    font-family: var(--font-family-sans);\n}\n.text-label {\n    font-size: var(--text-sm);\n    line-height: var(--leading-normal);\n    color: var(--gray-600);\n    font-family: var(--font-family-sans);\n    font-weight: 500;\n}\n.text-body {\n    font-size: var(--text-base);\n    line-height: var(--leading-relaxed);\n    color: var(--text-color);\n    font-family: var(--font-family-sans);\n}\n.text-heading {\n    font-size: var(--text-xl);\n    line-height: var(--leading-tight);\n    color: var(--gray-700);\n    font-family: var(--font-family-sans);\n    font-weight: 600;\n}\n.text-display {\n    font-size: var(--text-display);\n    line-height: var(--leading-tight);\n    color: var(--gray-700);\n    font-family: var(--font-family);\n    font-weight: 700;\n}\n/* Font family utilities */\n.font-sans {\n    font-family: var(--font-family-sans);\n}\n.font-mono {\n    font-family: var(--font-family-mono);\n}\n.font-display {\n    font-family: var(--font-family);\n}\n/* Font weight utilities */\n.font-normal {\n    font-weight: 400;\n}\n.font-medium {\n    font-weight: 500;\n}\n.font-semibold {\n    font-weight: 600;\n}\n.font-bold {\n    font-weight: 700;\n}\n/* Icon alignment utilities */\n.icon-text-align {\n    display: inline-flex;\n    align-items: center;\n    gap: var(--space-1);\n}\n.icon-text-align .material-symbols-outlined {\n    line-height: 1;\n    vertical-align: middle;\n}\n/* Flexible spacer that grows to take available space in a flex container */\n.spacer {\n    flex: 1;\n    width: 100%;\n    min-width: 0; /* allow it to shrink without causing overflow */\n}\n/* ================================ */\n/* BASE LAYOUT COMPONENTS           */\n/* ================================ */\n.main-container {\n    padding: var(--space-5);\n    max-width: var(--max-width);\n    margin: auto;\n    margin-top: var(--space-5);\n    display: flex;\n    min-height: 100vh;\n    flex-direction: column;\n    border: 1px solid #e5e7eb;\n    background: white;\n    border-radius: var(--border-radius-xl);\n    box-shadow: var(--box-shadow);\n}\n.content-area {\n    width: var(--content-width);\n    margin: 0 auto;\n}\n/* ================================ */\n/* CONTENT INFO BLOCK               */\n/* ===========================