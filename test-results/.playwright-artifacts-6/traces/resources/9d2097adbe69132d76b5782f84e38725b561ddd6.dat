n    font-weight: 500;\n    cursor: pointer;\n    transition: var(--transition);\n    white-space: nowrap;\n    text-decoration: none;\n}\n.btn:disabled {\n    opacity: 0.7;\n    cursor: not-allowed;\n    transform: none;\n}\n/* Primary button (orange) */\n.btn.primary {\n    background: var(--primary-color);\n    color: white;\n}\n.btn.primary:hover:not(:disabled) {\n    background: #e6900a;\n    transform: translateY(-1px);\n}\n/* Secondary button (blue) */\n.btn.secondary {\n    background: var(--secondary-color);\n    color: white;\n}\n.btn.secondary:hover:not(:disabled) {\n    background: #0284c7;\n    transform: translateY(-1px);\n}\n/* Tertiary button (outline/ghost) */\n.btn.tertiary {\n    background: var(--gray-50);\n    color: var(--gray-600);\n    border: 1px solid var(--gray-300);\n}\n.btn.tertiary:hover:not(:disabled) {\n    background: var(--gray-100);\n    color: var(--gray-700);\n    transform: translateY(-1px);\n}\n/* Small modifier */\n.btn.small {\n    padding: var(--space-1) var(--space-2);\n    margin: 0px;\n    font-size: var(--text-sm);\n    min-height: var(--space-7);\n}\n/* Icon-only small buttons */\n.btn.small.icon-only {\n    margin: 0px;\n    padding: var(--space-1);\n    width: var(--space-7);\n    height: var(--space-7);\n}\n.btn.small.icon-only .material-symbols-outlined {\n    font-size: var(--text-base);\n}\n/* Special styling for copy button in content info */\n.content-info__url-container .btn.small.icon-only {\n    width: var(--space-5);\n    height: var(--space-5);\n    padding: var(--space-1);\n}\n.content-info__url-container .btn.small.icon-only .material-symbols-outlined {\n    font-size: var(--text-xs);\n}\n/* Content info elements */\n.content-info__label {\n    color: var(--gray-500);\n    font-family: var(--font-family-sans);\n    font-size: var(--text-base);\n    font-weight: 500;\n}\n.content-info__title {\n    color: var(--primary-color);\n    font-family: var(--font-family-sans);\n    font-size: var(--text-lg);\n    